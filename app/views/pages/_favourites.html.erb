
  <ul id="favourites">
    <h3 class="group-title"> Your favourite groups </h3> <br><br>

    <% @favourites.each do |f| %>
      <%- if f.active == true %>
      <div>
      <li id="<%="#{f.id}"%>"><%= f.group_name %>
        <input id="box" type="checkbox" onClick="checkBox(event)">
      </li>
      <% end %>
    <% end %>

  </ul>
  <ul id="errors"></ul>
