

<h1> You made it Home </h1>


<!-- Links to sign in and out with Meetup -->
<% if current_user %>
  Welcome <%= current_user.name %>!
  <%= link_to "Sign Out", signout_path %>
<% else %>
  <%= link_to "Sign in with Meetup", "/auth/meetup" %>
<% end %>


  <ul>
  <% @groups.each do |group| %>
  <li><%= group["city"] %></li>
  <img src="<%= group["group_photo"]["thumb_link"] %>"><img>

    <ul>
    <%= group["topics"].each do |topic| %>
    <li><%= topic["name"] %></li>
    <% end %>
    </ul>
  <% end %>
  </ul>
